{
  "name": "entity-address",
  "version": "0.2.0",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n entity-address -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdrrichards/entity-address"
  },
  "author": {
    "name": "R Richards",
    "email": "rdrrichards@hotmail.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdrrichards/entity-address/issues"
  },
  "devDependencies": {
    "@angular/animations": "8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "8.0.0",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "3.3.8",
    "@types/node": "^10.12.30",
    "angular-in-memory-web-api": "^0.8.0",
    "codelyzer": "~4.5.0",
    "concurrently": "^3.6.1",
    "core-js": "^2.6.9",
    "del": "^3.0.0",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "gulp-rollup": "^2.16.2",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-typescript": "^4.1.0",
    "karma-typescript-angular2-transform": "^4.1.0",
    "node-sass": "^4.13.1",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.6.2",
    "primeng": "^7.1.3",
    "protractor": "~5.4.2",
    "rollup": "^1.13.0",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.5.2",
    "systemjs": "^3.1.6",
    "ts-node": "^8.0.3",
    "tslint": "~5.12.1",
    "typescript": "3.4.5",
    "zone.js": "^0.8.29"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "natives": "^1.1.6",
    "tsickle": "^0.34.3"
  }
